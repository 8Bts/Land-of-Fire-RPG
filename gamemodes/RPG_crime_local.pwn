				    GameTextForPlayer(i, string3, 2500, 3);
		  	 	}
			}
		}
		if(GetPVarInt(i, "Team") != 1 && GetPVarInt(i, "Job") == 24) SetPVarInt(i, "Job", 0);
//==============================================================================
	}
}


forward Timer1();
public Timer1()
{
	SetTimer("Timer1", 1038, 0);
	new jailed;
	for(new i=0; i<MAX_PLAYERS; i++)
	{
	    if(!IsPlayerConnected(i) || (!GetPVarInt(i,"Jailed") && !GetPVarInt(i, "On_Guard"))) continue;
	    if(GetPVarInt(i, "On_Guard") && !IsPlayerInArea(i, 44.5184, 1688.3832, 202.1263,1799.2560))
