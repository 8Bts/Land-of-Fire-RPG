stock gettimeEx(&hour=0, &minute=0, &second=0)
{
	new fhour, unixt;
	unixt = gettime(fhour, minute, second);
	if(fhour == 23) hour = 0;
	else hour = fhour+1;
	return unixt+3600;
}
