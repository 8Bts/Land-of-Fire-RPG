stock IsContainsBadSymbol(string[])
{
	if(strfind(string , "`" , true ) != -1
	|| strfind(string , "'" , true ) != -1
	|| strfind(string , "(" , true ) != -1
	|| strfind(string , ")" , true ) != -1
	|| strfind(string , "=" , true ) != -1
	|| strfind(string , ">" , true ) != -1
	|| strfind(string , "<" , true ) != -1
	|| strfind(string , ";" , true ) != -1
	|| strfind(string , ":" , true ) != -1
	|| strfind(string , "," , true ) != -1
	|| strfind(string , "|" , true ) != -1
	|| strfind(string , "\"" , true ) != -1
	|| strfind(string , "*" , true ) != -1) return 1;
	return 0;
}

