		{
            TextDrawHideForPlayer(i, prison_text);
            GangZoneStopFlashForAll(Prison_Gzone);
			DeletePVar(i, "pzonetext");
		}

		if(GetPlayerCameraMode(i) == 55 && GetPlayerState(i) == PLAYER_STATE_PASSENGER && !IsACop(i))
