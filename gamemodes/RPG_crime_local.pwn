    if(CheckFCalls(playerid, 24)) return 1;
    if(damagedid != INVALID_PLAYER_ID)
    {
		if(GetPVarInt(playerid, "nodm") == 1)
  		{
			if(GetPVarInt(playerid, "NoShowDialog") == 0)
			{
   				ShowPlayerDialog(playerid,39,DIALOG_STYLE_MSGBOX,"  ",RGB_WHITE"Bu ЏrazilЏrdЏ dќyљЌmЏk olmaz!\nSiz 2 saniyЏ mљddЏtinЏ donduruldunuz!","Оk","");
				SetPVarInt(playerid, "NoShowDialog", 1);
				SetTimerEx("HideDialogDM", 1500, false, "d", playerid);
				ApplyAnimation(playerid, "FAT", "IDLE_TIRED", 4.1, 1, 0, 0, 0, 1500, 1);
				SetCameraBehindPlayer(playerid);
			}
			else
			{
				SendClientMessage(playerid, COLOR_RED, "Burada dќyљЌmЏk qadaѓandЊr. ServerdЏn atЊldЊnЊz (Kick)");
				Kick_(playerid);
			    return 1;
			}
