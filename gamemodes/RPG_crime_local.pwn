  					pos[0] + 4.0 * floatcos(pos[3]-90.0, degrees),
     				pos[1] + 4.0 * floatsin(pos[3]-90.0, degrees),
       				pos[2] + 0.3);
		    }
		}
		if(GetPVarInt(i, "SpecTV") != -1)
		{
			new targetid = GetPVarInt(i, "SpecTV");
			if(IsPlayerConnected(targetid))
			{
   				if(GetPVarInt(i,"AdminLevel") >= 1)
			    {
	      			new Float:health;
