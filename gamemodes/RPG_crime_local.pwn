stock CheckRPname(name[])
{
	new isvalidname = 0;

	new namesize = strlen(name);
	new defisplace = 0; // ЉаЂделителљ имени и фамилии "_"

	switch(name[namesize-1])
	{
	    case '_', 'A'..'Z': return 0; // Последний сивол ника не может бытљ "_" и Ђаглавной буквой
	}

	for(new i; i < namesize; i++)
	{
    	switch(name[i])
		{
		    case '_':
			{
				isvalidname++;
				defisplace = i;
			}
		}
	}
	if(isvalidname != 1) return 0; // ПѓовеѓЏм пѓобел между именем и фамилией, он может бытљ толљко 1


	switch(name[0])
	{
	    case 'A'..'Z': {}
	    default: return 0; // пеѓвый символ ника должен бытљ с Ђаглавной буквой
	}

	if(defisplace+1 < namesize)
	{
		switch(name[defisplace+1])
		{
		    case 'A'..'Z': {}
		    default: return 0; // Пеѓвый символ фамилии толљко с Ђаглавной буквы
		}
	}

	for(new i = 1; i < defisplace; i++) // Пѓовеѓим ИмЏ, беЂ пеѓвой буквы
	{
	    switch(name[i])
	    {
	        case 'a'..'z': {}
	        default: return 0; // Все символы кѓоме пеѓвого в нике должны бытљ стѓочными, не допускаЌтсЏ дѓ. символы
		}
	}

	new validsurname = 0;
	

	for(new i = defisplace+1; i < namesize; i ++) // Пѓовеѓим фамилиЌ
	{
  		switch(name[i])
  		{
      		case 'a'..'z': {}
			case 'A'..'Z': validsurname++;
			default: return 0; // Если в фамилии естљ символы кѓоме букв
		}
	}
	if(validsurname == 0 || validsurname > 2) return 0; // можно 1 символ Ђаглавный помимо пеѓвой буквы в фамилии

	return 1;
}

stock SendPmMessage(color, string[], sender, toPlayer)
{
	new checkPm;
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		if(IsPlayerConnected(i) && !IsPlayerNPC(i))
		{
            checkPm = GetPVarInt(i, "readPmPlayer");
        	if(ReadPm[i] == 1 || checkPm == sender || checkPm == toPlayer) SendClientMessage(i, color, string);
		}
	}
}

