stock IsPlayerInArea(playerid, Float:min_x, Float:min_y ,Float:max_x, Float:max_y) // ОпѓеделЏет находитсЏ ли человек в квадѓате
{
	new Float:X, Float:Y, Float:Z;
	GetPlayerPos(playerid, X, Y, Z);
	if(X <= max_x && X >= min_x && Y <= max_y && Y >= min_y) return 1;
	return 0;
}
stock IsPlayerInCube(playerid, Float:xmin, Float:ymin, Float:zmin, Float:xmax, Float:ymax, Float:zmax)
{
	new Float:X, Float:Y, Float:Z;
	GetPlayerPos(playerid, X, Y, Z);
	if(X <= xmax && X >= xmin && Y <= ymax && Y >= ymin && Z <= zmax && Z >= zmin ) return 1;
	return 0;
}

stock PlayerInGangZone(playerid) // ОпѓеделЏет в какой ганг Ђоне находитсЏ игѓок
{
	new i;
	for(i = 1; i < sizeof(ZoneInfo); i++) if(IsPlayerInArea(playerid, ZoneInfo[i][zoneX], ZoneInfo[i][zoneY], ZoneInfo[i][zoneX2], ZoneInfo[i][zoneY2])) return i;
	return -1;
}


