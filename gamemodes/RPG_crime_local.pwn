public OnPlayerClickTextDraw(playerid, Text:clickedid)
{
	new stol = GetPVarInt(playerid,"Selected_Stol");
	new stol_max;
	new str[120];
	new text[10];
	new Float: x, Float: y, Float: z, Float: a;
	GetStolCpos(playerid, x, y, z, a);
	switch(stol)
	{
		case 0:{stol_max = stol_max_sf;}
		case 1:{stol_max = stol_max_ls;}
		case 2:{stol_max = stol_max_lv;}
	}
	if(clickedid == cb_textdraw[stol][0])
    {
        if(cursor == 0) cursor = stol_max;
		else cursor--;
		SetPVarInt(playerid, "Buyed_Color", 1);
		format(text, sizeof(text), "$%d", stol_prices[stol][cursor]);
		TextDrawSetString(cb_textdraw[stol][5], text);
		format(text, sizeof(text), "%d km/s", stol_speeds[stol][cursor]);
		TextDrawSetString(cb_textdraw[stol][6], text);
		/*format(text, sizeof(text), "%d ms(2)", stol_acc[stol][cursor]);
		TextDrawSetString(cb_textdraw[stol][7], text);*/
		format(text, sizeof(text), "%d/%d", cursor+1, stol_max+1);
		TextDrawSetString(cb_textdraw[stol][21], text);
		DestroyVehicle(cbstol[stol][MenuCar]);
		new menucar = CreateVehicle(stol_cars[stol][cursor] , x, y, z, a, 1, 1, -1);
		cbstol[stol][MenuCar] = menucar;
		SetPVarInt(playerid, "CarBuyCost", stol_prices[stol][cursor]);
		format(str, sizeof(str), "~y~%s", VehiclesName[stol_cars[stol][cursor]-400]);
		GameTextForPlayer(playerid, str, 990000, 6);
    }
    if(clickedid == cb_textdraw[stol][1])
    {
        if(cursor == stol_max) cursor = 0;
		else cursor++;
		SetPVarInt(playerid, "Buyed_Color", 1);
		format(text, sizeof(text), "$%d", stol_prices[stol][cursor]);
		TextDrawSetString(cb_textdraw[stol][5], text);
		format(text, sizeof(text), "%d km/s", stol_speeds[stol][cursor]);
		TextDrawSetString(cb_textdraw[stol][6], text);
		/*format(text, sizeof(text), "%d ms(2)", stol_acc[stol][cursor]);
		TextDrawSetString(cb_textdraw[stol][7], text);*/
		format(text, sizeof(text), "%d/%d", cursor+1, stol_max+1);
		TextDrawSetString(cb_textdraw[stol][21], text);
