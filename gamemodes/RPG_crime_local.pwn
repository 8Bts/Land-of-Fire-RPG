public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid, bodypart)
{
    if(CheckFCalls(playerid, 23)) return 1;
	switch(weaponid)
	{
	    case 22..34:
	    {
	        if(GetPVarInt(issuerid, "Shoot_Sound") == 1) PlayerPlaySound(issuerid, 6401, 0.0, 0.0, 0.0);
		}
	}
	if(GetPVarInt(playerid, "RegOnPaint") == 1 && GetPVarInt(issuerid, "RegOnPaint") == 1 && PaintOpenReg == false)
	{
		PaintList[GetPVarInt(issuerid, "OnPaint")][DamagePoint] += floatround(amount);
	}
	if(issuerid != INVALID_PLAYER_ID && bodypart == 9 && weaponid == 23 || weaponid == 34)
	{
		if(!GetPVarInt(playerid, "On_Guard") && GetPVarInt(playerid, "Jailed") != 2 )SetPlayerHealth(playerid, 0.0);
	}
	if(GetPVarInt(issuerid, "On_Guard") && weaponid == 33 && !GetPVarInt(playerid, "On_Guard"))
	{
	    TogglePlayerControllable(playerid, 0);
	    ApplyAnimation(playerid, "CRACK", "crckdeth2", 4.1, 1, 1, 1, 0, 12000);
	    SetTimerEx("pShock_Toggle", 15000, false, "i", playerid);
	    SendClientMessage(playerid, COLOR_RED,"GќzЏtЂi Ќok edici gљllЏ ilЏ sizЏ atЏЌ aЂdЊ");
	}
	return 1;
}
public OnPlayerGiveDamage(playerid, damagedid, Float: amount, weaponid, bodypart)
