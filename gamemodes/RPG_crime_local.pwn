	new sendername[MAX_PLAYER_NAME];
    new string[512];
    new loginmsg[256+1];
	for(new i; i < strlen(inputtext); i ++) if(inputtext[i] == '%') inputtext[i] = '#';
	switch(dialogid)
	{
		case 1:
		{
		    if(response)
		    {
		        for(new i; i < strlen(inputtext); i ++)
		        {
		            switch(inputtext[i])
		            {
		                case 'a'..'z', 'A'..'Z', '0'..'9': {}
