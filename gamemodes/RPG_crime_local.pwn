stock bool:CheckCnumberFormat(playerid,carn[], bool:report = false)
{
	if(strlen(carn)!=9)
	{
        if(report)SendClientMessage(playerid,COLOR_GREY,"Dљzgљn nќmrЏ formatЊ deyil! Dљzgљn nќmrЏyЏ nљmunЏ: [10-HD-333]");
		return false;
	}
	if(carn[2]!='-' || carn[5]!='-')
	{
	    if(report)SendClientMessage(playerid,COLOR_GREY,"Dљzgљn nќmrЏ formatЊ deyil! Dљzgљn nќmrЏyЏ nљmunЏ: [10-HD-333]");
		return false;
	}
	new first[3];
	strmid(first,carn,0,2);
	if(strcmp(first, "10"))
	{
        if(report)SendClientMessage(playerid,COLOR_GREY,"[ILK 2 RђQђM] Dљzgљn nќmrЏ formatЊ deyil! Dљzgљn nќmrЏyЏ nљmunЏ: [10-HD-333]");
		return false;
	}
	/*for	(new i; i<2; i++)
    {
		switch(first[i])
		{
	    	case '0'..'9': {}
	    	default: {
			if(report)SendClientMessage(playerid,COLOR_GREY,"Dљzgљn nќmre formatЊ deyil! Dљzgљn nќmreye numune: [10-HD-333]");
	    	return false; }
		}
	}*/
	new second[3];
	strmid(second,carn,3,5);
	for(new i=0; i<2; i++)
	{
		switch(second[i])
		{
			case 'A'..'H','J'..'P','R'..'U','X'..'Z': {}
			case 'Q','I','W': {
			if(report)SendClientMessage(playerid,COLOR_GREY,"'I', 'Q' vЏ 'W' simvollarЊndan istifadЏyЏ icazЏ verilmir!");
			return false;}
			default: {
			if(report)SendClientMessage(playerid,COLOR_GREY,"Dљzgљn nќmrЏ formatЊ deyil!\nDљzgљn nќmrЏyЏ nљmunЏ: [10-HD-333]");
	    	return false; }
		}
	}
	new third[4];
	strmid(third,carn,6,strlen(carn));

    for	(new i=0; i<3; i++)
    {
		switch(third[i])
		{
	    	case '0'..'9': {}
	    	default: {
			if(report)SendClientMessage(playerid,COLOR_GREY,"Dљzgљn nќmrЏ formatЊ deyil! Dљzgљn nќmrЏyЏ nљmunЏ: [10-HD-333]");
	    	return false; }
		}
	}
	if(strval(third)<=0)
	{
		if(report)SendClientMessage(playerid,COLOR_GREY,"Sonuncu 3 rЏqЏmin deyeri minimal halda '001' ola biler");
		return false;
	}
	return true;
}

