public CheckHouseIcons()
{
	SetTimer("CheckHouseIcons", 3000, false);
	new
	Float:x,
	Float:y,
	Float:z
	;
	new iconid;
	for(new i; i<MAX_PLAYERS; i++) //her oyuncu ucun
	{
		if(!IsPlayerConnected(i) || IsPlayerNPC(i) || ShowHouses[i] == 0) continue;
		iconid = 72;
		for(new house; house<700; house++) // yaxinda olan evlerin dovrle yoxlanmasi
		{
			if(iconid == 96) break;
			if(HouseInfo[house][hOwned] || HouseInfo[house][hEntrancez] > 800 ) continue;
			x = HouseInfo[house][hEntrancex];
            y = HouseInfo[house][hEntrancey];
            z = HouseInfo[house][hEntrancez];
			if(IsPlayerInRangeOfPoint(i, 150, x, y, z))
			{
				SetPlayerMapIcon(i, iconid, x, y, z, 31, 0, MAPICON_LOCAL);
				iconid++;
			}
		}
		for(new k = iconid; k<97; k++) // evvelki funk. cagirisindan qalan evlerin silinmesi
		{
            RemovePlayerMapIcon(i, k);
		}
	}
	return 1;
}
