		switch(weaponid)
		{
		    case 22..34:
		    {
				new Float:pos[3];
				GetPlayerPos(damagedid, pos[0], pos[1], pos[2]);

				if(GetPlayerDistanceFromPoint(playerid, pos[0], pos[1], pos[2]) > 5.0)
				{
			        if(GetPVarInt(playerid, "DelayAmmo") < gettimeEx())
			        {
			            new slot = ReturnSlot(weaponid);
			            if(PlayerAmmo[playerid][slot] > 0) PlayerAmmo[playerid][slot] --;
			            SetPVarInt(playerid, "DelayAmmo", gettimeEx()+1);
					}
				}
			}
			case 3:
			{
			    if(IsACop(playerid))
			    {
			        if(GetPVarInt(playerid, "NowCanTazer") < gettimeEx() && GetPVarInt(damagedid, "Cuff_Player") == 0 && !IsACop(damagedid))
			        {
			        	ApplyAnimation(damagedid, "CRACK", "crckdeth2", 4.1, 0, 1, 1, 1, 5000);
			        	SetPVarInt(playerid, "NowCanTazer", gettimeEx()+10);
			        	SetPVarInt(playerid, "Cuff_Player_Susp", damagedid);
			        	SendClientMessage(playerid, COLOR_WHITE, "Qandallamaq љЂљn ~k~~SNEAK_ABOUT~ dљymЏsini basЊn");
					}
					else SendClientMessage(playerid, COLOR_GREY, "CinayЏtkarЊ yerЏ yЊxmaq љЂљn gљЂљnљz ЂatmЊr");
			    }
			}
		}
    }
    return 1;
}

