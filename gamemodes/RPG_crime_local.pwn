     	mysql_fetch_field("FinishTime", tmp);
		getdate(year, month, day);
  		format(query, sizeof(query), "Siz "RGB_BLUE "[%d sЏviyyЏli] VIP oyunЂu "RGB_WHITE" kimi daxil oldunuz, VIP "RGB_BLUE"%s "RGB_WHITE" tarixinЏ qЏdЏr etibarlЊdЊr", GetPVarInt(playerid, "VipLevel"), tmp, year, month, day);
