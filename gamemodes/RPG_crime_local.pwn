			oSetPlayerPos(i, 830.645507,7.429725,1004.179687);
			SetPlayerTeam(i, i);
			ReplaceSkin(i);
			ResetPlayerWeaponsUAC(i);
			SetPlayerToTeamColor(i);
			TextDrawHideForPlayer(i, TDM_Count);
			SetPVarInt(i, "Exit_Type", 1);
		}
	}

	return 1;
}

