forward Pguard_Salary();
public Pguard_Salary()
{
    SetTimer("Pguard_Salary", 4000, 0);
	new hour, minute, sec, ptime, wptime;
	gettimeEx(hour, minute, sec);
	ptime = floatround(hour*3600 + minute*60 + sec);
	for(new i; i<4; i++)
	{
		if(AFKText[pGuards[i][ID]] < 4 && pGuards[i][STARTED])
		{
            if(ptime<pGuards[i][Ptime])	wptime = ptime + 86400 - pGuards[i][Ptime];
			else wptime = ptime - pGuards[i][Ptime];
			pGuards[i][STARTED] += (wptime * (2.77778));
			pGuards[i][TIME]+= wptime;
            pGuards[i][Ptime] = ptime;
		}
		else
		{
            pGuards[i][Ptime] = ptime;
		}
	}
}

stock GetCarPrice(model)
{
	new stol;
	new max_of;
	new price;
	switch(model)
	{
		case 418, 410, 404, 547, 554, 401, 527, 436, 540, 543, 549, 561, 458, 516, 551, 500, 526, 566, 496, 589, 405, 400, 507, 579, 439, 587, 536, 402:
		{
			stol = 0;
			max_of = stol_sf_size;
		}
		case 479, 467, 422, 492, 550, 466, 546, 529, 491, 489, 419, 474, 600, 585, 421, 517, 558, 576, 555, 542, 518, 565, 534, 412, 426, 602:
		{
            stol = 1;
            max_of = stol_ls_size;
		}
		case 559, 560, 506, 562, 480, 477, 415, 429, 541, 451, 411:
		{
            stol = 2;
            max_of = stol_lv_size;
		}
		default: return 0;
	}
	for(new i; i<max_of; i++)
	{
		if(model == stol_cars[stol][i]) price = stol_prices[stol][i];
	}
	return price;
}

