		}
	    jailed = GetPVarInt(i,"Jailed");
		if(GetPVarInt(i,"JailTime"))
		{
			GivePVarInt(i,"JailTime",-1);
			if(Pobegtime[i] == 1)
			{
				if((jailed == 1 && !IsPlayerInRangeOfPoint(i, 5.0,  264.6288,77.5742,1001.0391)) ||
				   (jailed == 2 && !( IsPlayerInCube(i, 80.1473, 1722.7605, 15.8565, 165.8735, 1763.0640, 19.8486 ) ||
				   					  IsPlayerInCube(i, 165.9402, 1710.0531, 15.8486, 172.3908, 1756.3954, 19.8486)  ||
