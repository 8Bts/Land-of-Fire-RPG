				if(strlen(inputtext) < 4 || strlen(inputtext) > 16)
				{
					return ShowPlayerDialog(playerid, 3, DIALOG_STYLE_MSGBOX, RGB_SERVER"Error","{ff0000}\
					Parolun uzunluѓu 4-16 iЌarЏdЏn ibarЏt olmalЊdЊr","Оk","");
